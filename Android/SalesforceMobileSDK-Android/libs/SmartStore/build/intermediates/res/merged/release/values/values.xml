        <item name="android:layout_margin">@dimen/sf__button_margin</item>
    </style>
    <style name="SalesforceSDK.AccountSwitcher.List">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="SalesforceSDK.ActionBarTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
