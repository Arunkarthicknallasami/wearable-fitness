<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="sf__inspector_button_background_color">#ccc</color>
    <color name="sf__inspector_cell_background_color">#eeeeee</color>
    <color name="sf__inspector_cell_text_color">#000000</color>
    <color name="sf__inspector_text_background_color">#eeeeee</color>
    <color name="sf__inspector_text_color">#00aa00</color>
    <dimen name="sf__inspector_cell_text_size">10sp</dimen>
    <dimen name="sf__inspector_grid_cell_spacing">1dp</dimen>
    <dimen name="sf__inspector_margin">1dp</dimen>
    <dimen name="sf__inspector_query_field_height">36dp</dimen>
    <dimen name="sf__inspector_text_size">10sp</dimen>
    <string name="app_name">SmartStore</string>
    <string name="app_package">com.salesforce.androidsdk.smartstore</string>
    <string name="sf__inspector_clear_button">Clear</string>
    <string name="sf__inspector_colon_button">:</string>
    <string name="sf__inspector_from_button">FROM</string>
    <string name="sf__inspector_indices_button">Indices</string>
    <string name="sf__inspector_indices_query">select soupName, path, columnType from soup_index_map</string>
    <string name="sf__inspector_left_bracket_button">{</string>
    <string name="sf__inspector_no_query_specified">No query specified</string>
    <string name="sf__inspector_no_rows_returned">No rows returned</string>
    <string name="sf__inspector_pageindex_hint">Page index (default: 0)</string>
    <string name="sf__inspector_pagesize_hint">Page size (default: 10)</string>
    <string name="sf__inspector_querytext_hint">Type your query here</string>
    <string name="sf__inspector_right_bracket_button">}</string>
    <string name="sf__inspector_run_button">Run</string>
    <string name="sf__inspector_select_button">SELECT</string>
    <string name="sf__inspector_soups_button">Soups</string>
    <string name="sf__inspector_soups_query">select soupName from soup_names</string>
    <string name="sf__inspector_where_button">WHERE</string>
    <style name="SmartStore">
    </style>
    <style name="SmartStore.Inspector">
    </style>
    <style name="SmartStore.Inspector.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/sf__inspector_button_background_color</item>
        <item name="android:layout_margin">@dimen/sf__inspector_margin</item>
    </style>
    <style name="SmartStore.Inspector.Cell">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/sf__inspector_cell_background_color</item>
        <item name="android:textColor">@color/sf__inspector_cell_text_color</item>
        <item name="android:textSize">@dimen/sf__inspector_cell_text_size</item>
        <item name="android:typeface">monospace</item>
    </style>
    <style name="SmartStore.Inspector.EditText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/sf__inspector_text_background_color</item>
        <item name="android:layout_margin">@dimen/sf__inspector_margin</item>
        <item name="android:textColor">@color/sf__inspector_text_color</item>
        <item name="android:textSize">@dimen/sf__inspector_text_size</item>
        <item name="android:typeface">monospace</item>
        <item name="android:gravity">top</item>
        <item name="android:inputType">textNoSuggestions</item>
    </style>
    <style name="SmartStore.Inspector.EditText.Query">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/sf__inspector_query_field_height</item>
    </style>
    <style name="SmartStore.Inspector.GridView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">@dimen/sf__inspector_margin</item>
        <item name="android:horizontalSpacing">@dimen/sf__inspector_grid_cell_spacing</item>
        <item name="android:verticalSpacing">@dimen/sf__inspector_grid_cell_spacing</item>
        <item name="android:listSelector">@null</item>
        <item name="android:stretchMode">columnWidth</item>
    </style>
</resources>